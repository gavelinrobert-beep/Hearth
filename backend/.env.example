# Database
DATABASE_URL="postgresql://noidchat:noidchat_password@localhost:5432/noidchat"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Server
PORT=3000
NODE_ENV="development"

# Llama API (for AI features)
LLAMA_API_URL="http://localhost:11434"

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR="./uploads"

# CORS
CORS_ORIGIN="http://localhost:5173"

# mediasoup (WebRTC Voice)
MEDIASOUP_WORKERS=1
MEDIASOUP_ANNOUNCED_IP=
RTC_MIN_PORT=10000
RTC_MAX_PORT=10100
